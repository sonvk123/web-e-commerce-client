seInt(value.priceProduct) * parseInt(value.quantity));\n    });\n\n    setTotal(sub_total);\n  }\n\n  //Hm ny dng  load d liu t API\n  //Khi ngi dng  ng nhp\n  useEffect(() => {\n    const fetchData = async () => {\n      if (localStorage.getItem(\"id_user\")) {\n        const params = {\n          idUser: localStorage.getItem(\"id_user\"),\n        };\n\n        const query = \"?\" + queryString.stringify(params);\n\n        console.log(query);\n\n        const response = await CartAPI.getCarts(query);\n        console.log(\"response:\", response);\n        setCart(response);\n\n        getTotal(response);\n      }\n    };\n\n    fetchData();\n\n    setLoadAPI(false);\n  }, [loadAPI]);\n\n  //Hm ny dng  truyn xung cho component con x v tr ngc d liu li component cha\n  const onDeleteCart = (getUser, getProduct) => {\n    console.log(\"idUser: \" + getUser + \", idProduct: \" + getProduct);\n\n    if (localStorage.getItem(\"id_user\")) {\n      // user  ng nhp\n\n      //Sau khi nhn c d liu  component con truyn ln th s gi API x l d liu\n      const fetchDelete = async () => {\n        const params = {\n          userId: getUser,\n          idProduct: getProduct,\n        };\n\n        const query = \"?\" + queryString.stringify(params);\n\n        const response = await CartAPI.deleteToCart(query);\n        console.log(response);\n      };\n\n      fetchDelete();\n\n      //Sau  thay i state loadAPI v load li hm useEffect\n      setLoadAPI(true);\n\n      alertify.set(\"notifier\", \"position\", \"bottom-left\");\n      alertify.error(\"Bn  Xa Hng Thnh Cng!\");\n    } else {\n      // user cha ng nhp\n\n      //Nu khng c phin lm vic ca Session User th mnh s x l vi Redux\n      const data = {\n        idProduct: getProduct,\n        idUser: getUser,\n      };\n\n      //a d liu vo Redux\n      const action = deleteCart(data);\n      dispatch(action);\n\n      alertify.set(\"notifier\", \"position\", \"bottom-left\");\n      alertify.error(\"Bn  Xa Hng Thnh Cng!\");\n\