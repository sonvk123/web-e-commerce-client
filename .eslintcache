a();\n      }\n    }\n  };\n  console.log(\"checkPush\", checkPush);\n  // Hm ny dng  a ht tt c carts vo API ca user\n  // Nu lc cha login m user c thm sn phm vo gi hng th khi login s a tt c sn phm  vo cart trn database\n  useEffect(() => {\n    const fetchData = async () => {\n      //Ln u s khng thc hin insert c v addCart = ''\n      if (checkPush === true) {\n        if (!listCart) {\n        } else {\n          listCart.map(async (value) => {\n            const params = {\n              idUser: localStorage.getItem(\"id_user\"),\n              idProduct: value.idProduct,\n              count: value.count,\n            };\n            console.log(\"params :\", params);\n            const query = \"?\" + queryString.stringify(params);\n\n            const response = await CartAPI.postAddToCart(query);\n            console.log(response);\n          });\n        }\n        setRedirect(true);\n      }\n    };\n\n    fetchData();\n  }, [checkPush]);\n\n  function validateEmail(email) {\n    const re =\n      /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\n    return re.test(String(email).toLowerCase());\n  }\n\n  function validatePassword(password) {\n    const isValid = password.length >= 8;\n    return isValid;\n  }\n\n  return (\n    <div className=\"limiter\">\n      <div className=\"container-login100\">\n        <div className=\"wrap-login100 p-l-55 p-r-55 p-t-65 p-b-50\">\n          <span className=\"login100-form-title p-b-33\">Sign In</span>\n\n          <div className=\"d-flex justify-content-center pb-5\">\n            {errorEmail && (\n              <span className=\"text-danger\">* Please Check Your Email</span>\n            )}\n            {errorPassword && (\n              <span className=\"text-danger\">* Please Check Your Password</span>\n            )}\n          </div>\n\n          <div className=\"wrap-input100 validate-inpu