ail(data);\n    };\n    fetchData();\n  }, [id]);\n\n  // ly d liu tt c sn phm  lc ra nhng sn phm cng loi\n  const [product, setProduct] = useState([]);\n\n  useEffect(() => {\n    const fetchProductData = async () => {\n      const response = await ProductAPI.getAPI();\n      const data = response.products.slice(0, 8);\n      setProduct(data);\n    };\n    fetchProductData();\n  }, []);\n\n  // hm ny  set s lng sn phm trong cart\n\n  const onChangeText = (e) => {\n    setText(e.target.value);\n  };\n\n  //Tng ln 1 n v\n  const upText = (count) => {\n    if (text <= count - 1) {\n      const value = parseInt(text) + 1;\n      setText(value);\n    } else {\n      window.alert(\"Vt qu s lng sn phm trong kho\");\n    }\n  };\n\n  //Gim 1 n v\n  const downText = () => {\n    const value = parseInt(text) - 1;\n\n    if (value === 0) return;\n\n    setText(value);\n  };\n\n  //Hm ny l Thm Sn Phm\n  const addToCart = async () => {\n    const currentQuantity = await getCartItemQuantity();\n    console.log(\"currentQuantity:\", currentQuantity);\n\n    if (text <= count && currentQuantity + text <= count) {\n      const id_user_cart = isLoggedIn()\n        ? localStorage.getItem(\"id_user\")\n        : id_user;\n\n      const data = {\n        idUser: id_user_cart,\n        idProduct: detail._id,\n        nameProduct: detail.name,\n        priceProduct: detail.price,\n        quantity: text,\n        img: detail.img1,\n      };\n\n      if (isLoggedIn()) {\n        const params = {\n          idUser: id_user_cart,\n          idProduct: detail._id,\n          quantity: text,\n        };\n        const query = \"?\" + queryString.stringify(params);\n        const req = await CartAPI.postAddToCart(query);\n        console.log(\"req:\", req);\n      } else {\n        const action = addCart(data);\n        dispatch(action);\n      }\n      setQuantity(currentQuantity + text);\n      setText(1);\n      alertify.set(\"notifier\", \"position\", \"bottom-left\");\n      alertify.succ